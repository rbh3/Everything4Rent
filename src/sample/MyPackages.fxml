      <ChoiceBox fx:id="pkg" layoutX="68.0" layoutY="69.0" nodeOrientation="RIGHT_TO_LEFT" prefHeight="31.0" prefWidth="330.0" />
      <Label alignment="CENTER" layoutX="174.0" layoutY="23.0" prefHeight="31.0" prefWidth="218.0" text="עריכה ומחיקת חבילות">
         <font>
            <Font name="System Bold" size="16.0" />
